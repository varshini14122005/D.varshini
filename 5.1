def common_characters(s1, s2, n):
    common_chars = []
    for char in s1:
        if char in s2 and char not in common_chars:
            common_chars.append(char)
            if len(common_chars) == n:
                break
    return ''.join(common_chars)

# Input
s1 = input().strip()
s2 = input().strip()
n = int(input().strip())

# Finding common characters
result = common_characters(s1, s2, n)

# Output
print(result)
